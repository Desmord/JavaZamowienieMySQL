<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="categoryPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.category.CategoryController">
	<children>
		<HBox prefHeight="580.0" prefWidth="1100.0">
			<AnchorPane prefHeight="580.0" prefWidth="900.0">
				<fx:include fx:id="editCategory" source="EditCategoryPane.fxml" />
				<fx:include fx:id="searchCategory" source="SearchCategoryPane.fxml" />
				<fx:include fx:id="newCategory" source="NewCategoryPane.fxml" />
				<fx:include fx:id="tableCategory" source="CategoryTable.fxml" />
			</AnchorPane>
			<VBox prefHeight="550.0" prefWidth="160.0">
            <children>
               <Button fx:id="showCategoryButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Pokaż Wszystkie">
                  <VBox.margin>
                     <Insets left="20.0" top="40.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="searchCategoryButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Wyszukaj">
                  <VBox.margin>
                     <Insets left="30.0" top="40.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="addCategoryButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Dodaj">
                  <VBox.margin>
                     <Insets left="30.0" top="15.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="editCategoryButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Edytuj">
                  <VBox.margin>
                     <Insets left="30.0" top="15.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="removeCategoryButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Usuń">
                  <VBox.margin>
                     <Insets left="30.0" top="15.0" />
                  </VBox.margin>
               </Button>
               <Label fx:id="infoLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="100.0" prefWidth="150.0" textAlignment="CENTER" textFill="#ae2020" wrapText="true">
                  <VBox.margin>
                     <Insets left="5.0" top="20.0" />
                  </VBox.margin>
               </Label>
            </children>
            <HBox.margin>
               <Insets left="30.0" top="10.0" />
            </HBox.margin></VBox>
		</HBox>
	</children>
</AnchorPane>
