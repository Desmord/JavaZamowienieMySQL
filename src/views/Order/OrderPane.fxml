<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="orderPane" disable="false"
	xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.order.OrderController">
	<children>
		<HBox prefHeight="600.0" prefWidth="1100.0">
			<children>
				<AnchorPane prefHeight="600.0" prefWidth="900.0">
					<HBox.margin>
						<Insets bottom="10.0" left="20.0" top="10.0" />
					</HBox.margin>
					<fx:include fx:id="editOrder" source="editOrderPane.fxml" />
					<fx:include fx:id="newOrder" source="newOrderPane.fxml" />
					<fx:include fx:id="orderTable" source="orderTable.fxml" />
					<fx:include fx:id="searchOrder" source="searchPane.fxml" />
				</AnchorPane>
				<VBox prefHeight="600.0" prefWidth="180.0">
					<HBox.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</HBox.margin>
					<children>
						<Button fx:id="showOrdersButton" mnemonicParsing="false"
							prefHeight="30.0" prefWidth="120.0" text="Pokaż zamówienia">
							<VBox.margin>
								<Insets left="20.0" top="40.0" />
							</VBox.margin>
						</Button>
						<Button fx:id="searchButton" mnemonicParsing="false"
							prefHeight="30.0" prefWidth="100.0" text="Wyszukaj">
							<VBox.margin>
								<Insets bottom="10.0" left="30.0" top="40.0" />
							</VBox.margin>
						</Button>
						<Button fx:id="insertButton" mnemonicParsing="false"
							prefHeight="30.0" prefWidth="100.0" text="Dodaj">
							<VBox.margin>
								<Insets left="30.0" top="5.0" />
							</VBox.margin>
						</Button>
						<Button fx:id="editButton" mnemonicParsing="false"
							prefHeight="30.0" prefWidth="100.0" text="Edytuj">
							<VBox.margin>
								<Insets left="30.0" top="15.0" />
							</VBox.margin>
						</Button>
						<Button fx:id="deleteButton" mnemonicParsing="false"
							prefHeight="30.0" prefWidth="100.0" text="Usuń">
							<VBox.margin>
								<Insets left="30.0" top="15.0" />
							</VBox.margin>
						</Button>
					</children>
				</VBox>
			</children>
		</HBox>
	</children>
</AnchorPane>
